                    if student[4]==val:
                        print(count, *student, sep="\t")
                        count += 1
                print("_________________________________________________________________________________________________________")

            elif search_menu=="6":
                val = input("Std code : ")
                system("cls")


                print("ID\tName\tFamily\tGender\tAge\tNcode\tStdcode\tClass\tPython\tPhp\tJava")
                print("_________________________________________________________________________________________________________")
                count = 1
                for student in student_list:
                    if student[5]==val:
                        print(count, *student, sep="\t")
                        count += 1
                print("_________________________________________________________________________________________________________")

            elif search_menu=="7":
                val = input("Class : ")
                system("cls")


                print("ID\tName\tFamily\tGender\tAge\tNcode\tStdcode\tClass\tPython\tPhp\tJava")
                print("_________________________________________________________________________________________________________")
                count = 1
                for student in student_list:
                    if student[6]==val:
                        print(count, *student, sep="\t")
                        count += 1
                print("_________________________________________________________________________________________________________")

            elif search_menu=="8":
                temp=False
            
            else:
                print("error")

    elif menu=="6":
        #region best java
        max_java_score = -1
        max_java_person =[]

        for student in student_list:

            if student[7]>max_java_score:
                max_java_score = student[7]

        for student in student_list:

            if student[7]==max_java_score:
                max_java_person.append(student[0])
                max_java_person.append("---")
                max_java_person.append(student[1])
                max_java_person.append("   ")
            student_name = "".join(max_java_person)


        print("max score in java : ", student_name, max_java_score)
        #endregion

    elif menu=="7":
        #region best python
        max_python_score = -1
        max_python_person =[]

        for student in student_list:

            if student[8]>max_python_score:
                max_python_score = student[8]

        for student in student_list:

            if student[8]==max_python_score:
                max_python_person.append(student[0])
                max_python_person.append("---")
                max_python_person.append(student[1])
                max_python_person.append("   ")
            student_name = "".join(max_python_person)


        print("max score in python : ", student_name,  max_python_score)
        #endregion

    elif menu=="8":
        #region best php
        max_php_score = -1
        max_php_person =[]

        for student in student_list:

            if student[9]>max_php_score:
                max_php_score = student[9]

        for student in student_list:

            if student[9]==max_php_score:
                max_php_person.append(student[0])
                max_php_person.append("---")
                max_php_person.append(student[1])
                max_php_person.append("   ")
            student_name = "".join(max_php_person)


        print("max score in php : ", student_name, max_php_score)
        #endregion

    elif menu=="9":
        #region best avg
        max_avg = -1
        max_avg_person = []
        
        for student in student_list:
            student_avg = (student[7] + student[8] + student[9]) / 3
            
            if student_avg > max_avg:
                max_avg = student_avg
                
        for student in student_list:
            student_avg = (student[7] + student[8] + student[9]) / 3
            
            if student_avg == max_avg:
                max_avg_person.append(student[0])
                max_avg_person.append(student[1])
                max_avg_person.append(max_avg)

        print("max avg : ", max_avg_person)
        #endregion
    

    else:
        continue_ = False
