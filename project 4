from os import system
from random import randint

continue_ = True
student_list = []

while continue_ :
    
    #region get menu
    flag = True

    while flag:

        print("\n---------------------")
        print("1. Add student")
        print("2. Show student")
        print("3. Remove student")
        print("4. Edit student")
        print("5. Search student")
        print("6. best java")
        print("7. best python")
        print("8. best php")
        print("9. best avg")
        print("10. Exit")
