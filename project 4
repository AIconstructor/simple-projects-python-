            flag = False
        else:
            print("error")
    #endregion
    
    if menu=="1":

        temp = True

        while temp:
            system("cls")

            #region get name

            flag = True

            while flag:
                name = input("please enter name : ")
                system("cls")

                if name =="":
                    print("error! name is empty")
                else:
                    flag = False

            #endregion

            #region get family
            flag = True

            while flag:
                family = input("please enter family : ")
                system("cls")

                if family =="":
                    print("error! family is empty")
                else:
                    flag = False
            #endregion
                    
            #region get gender
            flag = True

            while flag:
                gender = input("please enter gender : ")
                system("cls")

                if gender not in ["male", "female", "other"] :
                    print("error! gender not in male, female, other")
                else:
                    flag = False
            #endregion

            #region get age
            flag = True

            while flag:
                age = int(input("please enter age : "))
                system("cls")

                if age not in range(5, 121):
                    print("error! age is not in range[5, 120]")
                else:
                    flag = False
            #endregion

            #region get ncode
            flag = True

            while flag:
                ncode = input("please enter ncode : ")
                system("cls")

                if ncode =="":
                    print("error! ncode is empty")
                else:

                    find_student=False

                    for student in student_list:
                        if student[4]==ncode:
                            find_student=True
                    
                    if find_student:
                        print("Error!!!", ncode, "exists")

                    else:
                        flag = False
            #endregion     

            #region get stdcode
            flag = True

            while flag:
                stdcode = input("please enter stdcode : ")
                system("cls")

                if stdcode =="":
                    print("error! stdcode is empty")
                else:

                    find_student=False

                    for student in student_list:
                        if student[5]==stdcode:
                            find_student=True
                    
